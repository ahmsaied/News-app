<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bottlesecret.news.newsapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ABCNewsActivity"
            android:label="ABC NEWS" />
        <activity
            android:name=".TheWashingtonPostActivity"
            android:label="The Washington Post" />
        <activity
            android:name=".FoxNewsActivity"
            android:label="FOX News" />
        <activity
            android:name=".CBSNewsActivity"
            android:label="CBS News" />
        <activity
            android:name=".BBCNewsActivity"
            android:label="BBC News" />
        <activity
            android:name=".CNNNewsActivity"
            android:label="CNN News" />
        <activity android:name=".details" />
    </application>

</manifest>